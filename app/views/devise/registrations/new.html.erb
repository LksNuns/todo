<div class="col-md-4 col-md-offset-4">
  <h2 class="form-signin-heading">Cadastrar</h2>
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <%= devise_error_messages! %>
    <%= f.label :email, class: "sr-only" %>
    <%= f.email_field :email, id: "inputEmail", class: "form-control", autofocus: true, placeholder: "teste@teste.com" %>
    <%= f.label :password, class: "sr-only" %>
    <%= f.password_field :password, id:"inputPassword", class:"form-control", autocomplete: "off", placeholder: "password" %>
    <%= f.password_field :password_confirmation, id:"inputPassword", class:"form-control", autocomplete: "off", placeholder: "password" %>
    <button class="btn btn-lg btn-primary btn-block" type="submit">Cadastrar</button>
  <% end %>
</div>
